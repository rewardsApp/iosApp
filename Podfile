# Uncomment the next line to define a global platform for your project
 platform :ios, '11.4'

target 'Rewardsapp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Rewardsapp
pod 'FacebookCore'
pod 'FacebookLogin'
pod 'GoogleSignIn'

  target 'RewardsappTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RewardsappUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
